{
  "extends": "./base.json",
  "compilerOptions": {
    "incremental": false,
    "lib": [
      "ES2023"
    ],
    "target": "ES2022",
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}